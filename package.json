{
    "name": "kejinshou-vue",
    "version": "1.2.0",
    "private": true,
    "description": "Kejinshou H5",
    "scripts": {
        "start:local": "vite --mode=self",
        "start:test": "vite --mode=test",
        "start:dev": "vite --mode=dev",
        "start:prod": "vite --mode=prod",
        "build:test": "vite build --mode=test",
        "build:prod": "vite build --mode=prod",
        "build:dev": "vite build --mode=dev",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript",
        "Starter"
    ],
    "web-types": "src/utils/types/web-types.json",
    "author": "duoli",
    "license": "MIT",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "@popjs/util": "^0.3.1",
        "@types/node": "^16.7.13",
        "@vant/use": "^1.3.0",
        "ant-design-vue": "^2.2.2",
        "axios": "^0.21.1",
        "crypto-js": "^4.0.0",
        "dayjs": "^1.10.6",
        "dotenv": "^10.0.0",
        "element-plus": "^1.1.0-beta.9",
        "lodash-es": "^4.17.21",
        "ua-parser-js": "^0.7.21",
        "vue": "^3.0.5",
        "vue-router": "^4.0.6",
        "vue-transitions-css": "^0.0.2",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@types/crypto-js": "^4.0.2",
        "@types/lodash-es": "^4.17.4",
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "@vitejs/plugin-vue": "^1.2.1",
        "@vue/compiler-sfc": "^3.0.5",
        "@vue/test-utils": "^2.0.0-rc.4",
        "better-mock": "^0.3.2",
        "eslint": "^7.24.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-vue": "^7.8.0",
        "less": "^4.1.1",
        "lint-staged": "^11.0.0",
        "typescript": "^4.1.3",
        "vite": "^2.1.5",
        "vite-plugin-components": "^0.13.2",
        "vite-plugin-style-import": "^1.0.0",
        "vue-tsc": "^0.3.0"
    }
}
